{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug test2 (malloc_2.cpp + test2.cpp)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/test2",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
